<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="45"
                android:endColor="#D1EFB5"
                android:startColor="#552800"
                android:centerColor="#BA7600"/>
        </shape>
    </item>
    <item android:top="3dp" android:bottom="3dp" android:left="3dp" android:right="3dp">
        <shape android:shape="oval">
            <solid android:color="#282f32" />
        </shape>
    </item>
    <item
        android:bottom="15dp"
        android:top="15dp"
        android:left="15dp"
        android:right="15dp"
        android:drawable="@drawable/image"
        />
</layer-list>