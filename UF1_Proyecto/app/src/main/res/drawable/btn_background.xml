<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="50dp" />
            <gradient
                android:angle="360"
                android:type="linear"
                android:startColor="#552800" />
            <gradient
                android:angle="360"
                android:type="linear"
                android:centerColor="#BA7600" />
            <gradient
                android:angle="360"
                android:type="linear"
                    android:endColor="#D1EFB5" />
            <size
                android:width="24dp"
                android:height="24dp" />
        </shape>
    </item>
    <item android:top="4dp" android:bottom="4dp" android:left="4dp" android:right="4dp">>
        <shape android:shape="rectangle">
            <corners android:radius="50dp" />
            <solid android:color="#FFFFB0" />
        </shape>
    </item>
</layer-list>